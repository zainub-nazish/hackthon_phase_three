{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://zainubnazish-phase-three-huggies.hf.space"
  },
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "https://zainubnazish-phase-three-huggies.hf.space/api/v1/:path*"
    }
  ]
}
